<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nosix.manager.dao.UserDao">
    <select id="getUserByUsername" resultType="com.nosix.manager.bean.User" parameterType="java.lang.String">
        SELECT * FROM ns_user WHERE NSusername = #{username}
    </select>
</mapper>